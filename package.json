{
    "name": "tbc-members-core-ui",
    "version": "0.3.0",
    "scripts": {
        "build": "webpack --mode production",
        "build:dev": "webpack --mode development",
        "build:start": "cd dist && PORT=3000 npx serve",
        "start": "webpack serve --open --mode development",
        "start:live": "webpack serve --open --mode development --live-reload --hot",
        "fmt": "prettier --config prettier.config.js --write .",
        "lint": "eslint --ext js,jsx src/",
        "lint:fix": "eslint --ext js,jsx --fix src/",
        "lint:staged": "lint-staged",
        "fix": "npm run fmt && npm run lint:fix",
        "prepare": "husky install"
    },
    "license": "MIT",
    "author": {
        "name": "Tech by Choice Paid Source",
        "email": "paidsource@techbychoice.org"
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "prettier --config prettier.config.js --write",
            "eslint --fix"
        ],
        "*.{css,gql,graphql,html,json,less,md,mdx,sass,scss}": [
            "prettier --config prettier.config.js --write"
        ],
        "*.{yaml,yml}": [
            "prettier --config prettier.config.js --tab-width 2 --write"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.23.6",
        "@babel/plugin-transform-runtime": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.23.3",
        "@emotion/react": "latest",
        "@emotion/styled": "latest",
        "@mui/icons-material": "^5.14.8",
        "@mui/material": "latest",
        "autoprefixer": "^10.1.0",
        "babel-loader": "^8.3.0",
        "babel-preset-es2015": "^6.24.1",
        "css-loader": "^6.3.0",
        "eslint": "^8.56.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "postcss": "^8.2.1",
        "postcss-loader": "^4.1.0",
        "prettier": "^3.1.0",
        "react-quill": "^0.0.2",
        "react-router": "latest",
        "react-router-dom": "^6.15.0",
        "react-scripts": "^3.0.1",
        "style-loader": "^3.3.0",
        "webpack": "^5.57.1",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.3.1"
    },
    "dependencies": {
        "@babel/runtime": "^7.13.10",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "@tiptap/extension-heading": "^2.1.13",
        "@tiptap/extension-paragraph": "^2.1.13",
        "@tiptap/extension-text": "^2.1.13",
        "@tiptap/pm": "^2.1.13",
        "@tiptap/react": "^2.1.13",
        "@tiptap/starter-kit": "^2.1.13",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "dotenv-webpack": "^8.0.1",
        "eslint-webpack-plugin": "^4.0.1",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^2.7.6",
        "quill": "^1.3.7",
        "react": "^18.2.0",
        "react-dev-utils": "^12.0.1",
        "react-dom": "^18.2.0",
        "workbox-webpack-plugin": "^7.0.0"
    },
